// .vscode/launch.json
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "[LOCAL] Lend (Firebase Emulators)",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define=ENV=local"]
      // "preLaunchTask": "local-with-emulators"
    },
    {
      "name": "[DEV] Lend",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define=ENV=dev"]
    },
    {
      "name": "[PROD] Lend",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define=ENV=prod"]
    }
  ]
}
